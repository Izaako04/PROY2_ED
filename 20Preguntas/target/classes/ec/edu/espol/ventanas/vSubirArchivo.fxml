<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ec.edu.espol.ventanas.VSubirArchivoController">
    <stylesheets>
        <URL value="@/styles/vsubirarchivo.css" />
    </stylesheets>
   <children>
      <Label layoutX="202.0" layoutY="51.0" text="Antes de comenzar..." textFill="#37861b">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <Button layoutX="193.0" layoutY="279.0" mnemonicParsing="false" text="O jugar con archivos precargados" />
      <Label layoutX="101.0" layoutY="133.0" text="Subir arhivo de preguntas" />
      <Label layoutX="336.0" layoutY="133.0" text="Subir arhivo de respuestas" />
      <ImageView fx:id="archivoPreguntas" fitHeight="72.0" fitWidth="114.0" layoutX="142.0" layoutY="164.0" onMouseClicked="#subirArchivoPreguntas" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../imagenes/hojaAnimada_vSubirArchivo.jpg" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fx:id="archivosRespuestas" fitHeight="72.0" fitWidth="114.0" layoutX="385.0" layoutY="164.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../imagenes/hojaAnimada_vSubirArchivo.jpg" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
   </children>
</AnchorPane>
